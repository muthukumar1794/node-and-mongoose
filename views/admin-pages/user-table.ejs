<%- include('../includes/header.ejs') %>
<div>
<h1 class="page-header text-center" style="padding:30px">Latest Launches</h1>
<%if(users.length>0){%>
<div class="col-sm-12">
  <%for(let user of users){%> 


<div class="col-sm-3" style="float:left">
    <div class="product-image-wrapper">
      <div class="single-products">
        <div class="productinfo text-center" style="padding: 30px 0;">
            <div>
                <p style="padding: 15px 0 0 0"><%=user.email%></p>
        </div>
    
        <p style="padding: 15px 0 0 0"><%=user.password%></p>
        
         
        
          <form action="/admin/product/details/<%=user._id%>" method="post">
          <button  class="btn btn-secondary add-to-cart"><i class="fa fa-shopping-cart"></i>Details</button>
        </form>
        </div>
      </div>
    </div>
  </div>
   
    <%}%>
</div>
<%} else {%>
    <h4 class="text-center" >No User added yet</h4>
            <%}%>
</div>
<%- include('../includes/end.ejs') %>